{
    "rewrites": [
        {
            "source": "/models/:path*",
            "destination": "/public/models/:path*"
        }
    ],
    "headers": [
        {
            "source": "/models/:path*",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "model/gltf-binary"
                }
            ]
        }
    ]
}